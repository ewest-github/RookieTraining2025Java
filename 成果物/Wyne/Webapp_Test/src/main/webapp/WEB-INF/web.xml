<?xml version="1.0" encoding="UTF-8"?>

<web-app>
	<display-name>Attendance System</display-name>
	<welcome-file-list>
		<welcome-file>time_manager.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
	<servlet-name>AttendanceServlet</servlet-name> 
	<servlet-class>attendance.attendance</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AttendanceServlet</servlet-name>
		<url-pattern>/attendance</url-pattern>
	</servlet-mapping>
	<servlet>
    <servlet-name>AttendTimeInsertServlet</servlet-name>
    <servlet-class>attendance.AttendTimeInsertServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>AttendTimeInsertServlet</servlet-name>
    <url-pattern>/insertAttendTime</url-pattern>
</servlet-mapping>
	
	
</web-app>